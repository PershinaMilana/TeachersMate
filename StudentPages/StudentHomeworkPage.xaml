<Page x:Class="TeachersMate.StudentPages.StudentHomeworkPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TeachersMate.StudentPages"
      mc:Ignorable="d" 
       d:DesignHeight="556" d:DesignWidth="910"
      Title="HomeworkPage">

    <Grid Background="#F2E5E5" Margin="50,0,20,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Homeworks:" Grid.Row="0" VerticalAlignment="Center" FontFamily="Roboto" Foreground="#665A48" FontSize="17" Margin="10"/>
        <StackPanel x:Name="homeworkStackPanell" Grid.Row="1" Margin="10"/>
    </Grid>
</Page>
